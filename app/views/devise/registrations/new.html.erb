<div id='sign-in-wrapper'>
<h1>#TODO</h1>
<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
<br/><%= f.email_field :email, autofocus: true, placeholder: 'Email', class: 'sign-in-input' %>
<br/><%= f.password_field :password, placeholder: 'Password', class: 'sign-in-input' %>
<br/><%= f.password_field :password_confirmation, placeholder: 'Confirm password', class: 'sign-in-input' %>
<br/><br/><%= f.submit "Sign up",  class: 'sign-in-submit' %>
<% end %>

<%= render "devise/shared/links" %>
</div>